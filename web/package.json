{
  "name": "sentence-collector-web",
  "version": "0.0.1",
  "description": "A web console for collecting and validating sentences",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "start": "./node_modules/webpack/bin/webpack.js --watch --mode development",
    "build": "./node_modules/webpack/bin/webpack.js --mode production",
    "lint": "eslint src tests",
    "test": "npm run lint && jest",
    "test:unit": "jest"
  },
  "dependencies": {
    "connected-react-router": "^6.9.1",
    "hammerjs": "^2.0.8",
    "iso-639-1": "^2.1.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-react": "^7.12.13",
    "@babel/runtime": "^7.13.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^8.0.0",
    "css-loader": "^5.1.1",
    "eslint": "^7.21.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-enzyme": "^0.2.0",
    "eslint-plugin-react": "^7.22.0",
    "file-loader": "^6.2.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "mini-css-extract-plugin": "^1.3.9",
    "react-test-renderer": "^17.0.1",
    "redux-mock-store": "^1.5.3",
    "style-loader": "^2.0.0",
    "webpack": "^5.24.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/dist/**",
      "!**/node_modules/**",
      "!**/tests/**",
      "!**/src/components/swipecard/**",
      "!webpack.config.js"
    ],
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 82,
        "lines": 93,
        "statements": 91
      }
    },
    "transform": {
      "^.+\\.js$": "babel-jest",
      "\\.svg$|\\.css$": "<rootDir>/tests/file-transformer.js"
    },
    "setupFiles": [
      "./tests/setup.js"
    ]
  }
}
